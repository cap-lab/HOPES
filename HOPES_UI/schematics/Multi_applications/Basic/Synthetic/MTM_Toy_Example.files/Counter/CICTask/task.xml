<?xml version="1.0" encoding="utf-8"?>

<cic_manual name="MTM_Toy_Example">
  <setting_list>
    <setting cycle="10" unit="us" FuncTarget="PTHREAD" ThreadSetting="MULTI_THREAD" TaskGraphProperty="ProcessNetwork" />
  </setting_list>
  <task_list>
    <task name="Sender" type="Computational">
      <config>
        <location x="232" y="160" width="80" height="40" />
        <parameter name="CICFile" value="Sender.cic" />
        <parameter name="RunCondition" value="time-driven" />
        <parameter name="HasInternalStates" value="Yes" />
        <parameter name="Deadline" value="1" />
        <parameter name="DeadlineUnit" value="us" />
        <parameter name="Period" value="1" />
        <parameter name="PeriodUnit" value="us" />
        <parameter name="Priority" value="1" />
        <parameter name="Language" />
        <parameter name="CompileFlags" value="" />
        <parameter name="LinkFlags" value="" />
        <parameter name="HasMTM" value="No" />
        <parameter name="HasSubGraph" value="No" />
        <parameter name="IsDependency" value="No" />
        <parameter name="LoopType" value="NONE" />
        <parameter name="LoopCount" value="0" />
        <parameter name="IsLoopSubTask" value="No" />
        <parameter name="IsDesignatedTask" value="No" />
        <parameter name="SubgraphProperty" value="Dataflow" />
      </config>
      <inport_list />
      <outport_list>
        <port id="0" name="output" type="fifo" sample_size="4" sample_rate="Multiple">
          <rate id="0" mode="Inc_Rate_1" rate="1" />
          <rate id="1" mode="Dec_Rate_10" rate="10" />
        </port>
      </outport_list>
      <portMap_list />
      <libraryPort_list />
      <extraSetting_list />
      <control priority="0" controller="true">
        <control_params />
        <slaves />
      </control>
    </task>
    <task name="Receiver" type="Computational">
      <config>
        <location x="472" y="160" width="80" height="40" />
        <parameter name="CICFile" value="Receiver.cic" />
        <parameter name="RunCondition" value="data-driven" />
        <parameter name="HasInternalStates" value="Yes" />
        <parameter name="Deadline" value="1" />
        <parameter name="DeadlineUnit" value="us" />
        <parameter name="Period" value="1" />
        <parameter name="PeriodUnit" value="us" />
        <parameter name="Priority" value="1" />
        <parameter name="Language" />
        <parameter name="CompileFlags" value="" />
        <parameter name="LinkFlags" value="" />
        <parameter name="HasMTM" value="No" />
        <parameter name="HasSubGraph" value="No" />
        <parameter name="IsDependency" value="No" />
        <parameter name="LoopType" value="NONE" />
        <parameter name="LoopCount" value="0" />
        <parameter name="IsLoopSubTask" value="No" />
        <parameter name="IsDesignatedTask" value="No" />
        <parameter name="SubgraphProperty" value="Dataflow" />
      </config>
      <inport_list>
        <port id="0" name="input" type="fifo" sample_size="4" sample_rate="Fixed">
          <rate id="0" mode="Default" rate="1" />
        </port>
      </inport_list>
      <outport_list />
      <portMap_list />
      <libraryPort_list />
      <extraSetting_list />
      <control priority="0" controller="true">
        <control_params />
        <slaves />
      </control>
    </task>
  </task_list>
  <library_list />
  <channel_list>
    <channel src_task="Sender" src_port="output" dst_task="Receiver" dst_port="input" size="10" init_size="0" sampleType="" sampleSize="4">
      <edge_list>
        <edge id="0" x="312" y="180" />
        <edge id="1" x="472" y="180" />
      </edge_list>
    </channel>
  </channel_list>
  <taskLibraryConnection_list />
  <libraryLibraryConnection_list />
  <taskGroupList name="taskGroups" />
</cic_manual>

